        <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Handle layout adjustments for Google Translate bar
            const observer = new MutationObserver((mutations) => {
                mutations.forEach((mutation) => {
                    if (mutation.type === 'attributes' && mutation.attributeName === 'class') {
                        const header = document.querySelector('.header');
                        const navbar = document.querySelector('.navbar');
                        
                        if (document.body.classList.contains('translated-ltr') || 
                            document.body.classList.contains('translated-rtl')) {
                            header.style.top = '40px';
                            navbar.style.top = '100px';
                        } else {
                            header.style.top = '0';
                            navbar.style.top = '60px';
                        }
                    }
                });
            });

            observer.observe(document.body, {
                attributes: true,
                attributeFilter: ['class']
            });

            // Call Us Dropdown
            const callUsBtn = document.getElementById('call-us-btn');
            const callUsDropdown = document.querySelector('.call-us-dropdown');

            if (callUsBtn && callUsDropdown) {
                callUsBtn.addEventListener('click', function(e) {
                    e.stopPropagation();
                    callUsDropdown.classList.toggle('show');
                });

                document.addEventListener('click', function(e) {
                    if (!e.target.closest('.call-us')) {
                        callUsDropdown.classList.remove('show');
                    }
                });
            }

            // Language selection dropdown
            const langSelect = document.querySelector('.lang-select button');
            const langDropdown = document.getElementById('langDropdown');

            if (langSelect && langDropdown) {
                langSelect.addEventListener('click', function(e) {
                    e.stopPropagation();
                    langDropdown.style.display = langDropdown.style.display === 'block' ? 'none' : 'block';
                });

                document.addEventListener('click', function(e) {
                    if (!e.target.closest('.lang-select')) {
                        langDropdown.style.display = 'none';
                    }
                });
            }

            // Navbar scroll handling
            const navbar = document.getElementById('navbar');
            let lastScrollTop = 0;

            window.addEventListener('scroll', () => {
                const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                if (scrollTop > lastScrollTop) {
                    navbar.style.top = '-70px'; // Hide navbar
                } else {
                    navbar.style.top = document.body.classList.contains('translated-ltr') ? '100px' : '60px'; // Show navbar
                }
                lastScrollTop = scrollTop;
            });

            // Add user profile dropdown handling
            const userProfile = document.querySelector('.user-profile');
            const userDropdown = userProfile?.querySelector('.user-dropdown');
            
            if (userProfile && userDropdown) {
                // Handle hover events
                userProfile.addEventListener('mouseenter', function() {
                    userDropdown.style.display = 'block';
                });
                
                userProfile.addEventListener('mouseleave', function() {
                    userDropdown.style.display = 'none';
                });

                // Handle click events
                document.addEventListener('click', function(e) {
                    if (!userProfile.contains(e.target)) {
                        userDropdown.style.display = 'none';
                    }
                });

                // Prevent dropdown from closing when clicking inside it
                userDropdown.addEventListener('click', function(e) {
                    e.stopPropagation();
                });
            }
        });

        function selectLanguage(langCode, langText) {
            document.getElementById("selected-lang").textContent = langText;
            var translateSelect = document.querySelector(".goog-te-combo");
            if (translateSelect) {
                translateSelect.value = langCode;
                translateSelect.dispatchEvent(new Event("change"));
            }
            document.getElementById("langDropdown").style.display = "none";
        }

        function googleTranslateElementInit() {
            new google.translate.TranslateElement({
                pageLanguage: 'en',
                includedLanguages: 'en,mr,hi,gu,ta,te,pa,bn',
                autoDisplay: false,
                layout: google.translate.TranslateElement.InlineLayout.SIMPLE
            }, 'google_translate_element');
        }

        // Add hero image slider functionality
        const images = document.querySelectorAll('.hero img');
        const textContainer = document.querySelector('.text');
        const titleElement = document.querySelector('.text h1');
        const descriptionElement = document.querySelector('.text p');

        const texts = [
            {
                title: "Let's Adventure Beyond Limits! ðŸŒâœˆï¸",
                description: "Explore new destinations with AI-powered travel planning."
            },
            {
                title: "Wander More, Worry Less! ðŸŽ’ðŸŒŸ",
                description: "Personalized itineraries, smart packing lists, and travel tipsâ€”all in one place."
            },
            {
                title: "Your Journey, Your Way! ðŸš€ðŸ—ºï¸",
                description: "Plan, pack, and explore effortlessly with Travenner."
            }
        ];

        let currentIndex = 0;

        function changeSlide() {
            images[currentIndex].classList.remove('active');
            textContainer.classList.remove('active');

            currentIndex = (currentIndex + 1) % images.length;

            setTimeout(() => {
                titleElement.textContent = texts[currentIndex].title;
                descriptionElement.textContent = texts[currentIndex].description;

                images[currentIndex].classList.add('active');
                textContainer.classList.add('active');
            }, 500);
        }

        setInterval(changeSlide, 5000);
    </script>   
   

